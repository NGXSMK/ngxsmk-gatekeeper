{
  "name": "ngxsmk-gatekeeper",
  "version": "1.1.0",
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "cli:build": "cd tools/cli && npm run build",
    "cli:watch": "cd tools/cli && npm run watch",
    "prepublishOnly": "npm run build",
    "publish:lib": "npm run build && cd dist/ngxsmk-gatekeeper && npm publish",
    "publish:lib:dry-run": "npm run build && cd dist/ngxsmk-gatekeeper && npm publish --dry-run",
    "pack:lib": "npm run build && cd dist/ngxsmk-gatekeeper && npm pack"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/platform-browser": "^20.3.0",
    "@angular/router": "^20.3.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-devkit/schematics": "^20.3.0",
    "@angular-devkit/schematics-cli": "^20.3.0",
    "@angular/build": "^20.3.13",
    "@angular/cli": "^20.3.9",
    "@angular/compiler-cli": "^20.3.0",
    "@schematics/angular": "^20.3.0",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^25.0.3",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^20.3.0",
    "typescript": "~5.9.2",
    "vitepress": "^1.6.4",
    "zone.js": "~0.15.0"
  }
}